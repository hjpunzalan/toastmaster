{
  "name": "server",
  "version": "1.0.0",
  "description": "An API for Southern River Toastmasters",
  "main": "app.js",
  "scripts": {
    "start": "nodemon server.js",
    "server": "nodemon server.js",
    "start:prod": "set NODE_ENV=production&& nodemon server.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --pregit fix client"
  },
  "author": "Jonathan Punzalan",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.521.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.2",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.0.0",
    "generate-password": "^1.4.2",
    "helmet": "^3.20.0",
    "hpp": "^0.2.2",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.9",
    "mongoose-autopopulate": "^0.9.1",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.0",
    "pug": "^2.0.4",
    "validator": "^11.1.0",
    "xss-clean": "^0.1.1"
  }
}